public class SetExample{
    private Set<String> usernames = new Set<String>();
    
    public void isUnique(String name){
        if(usernames.contains(name)){
            System.debug('Not Unique');
            return;
        }
        
        usernames.add(name);
     	System.debug('Unique');
    }
   
}


